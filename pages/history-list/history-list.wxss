.haeder {
  position: relative;
  height: 200rpx;
  display: flex;
  align-items: center;
}
.haeder .bg-img {
  position: absolute;
  width: 100%;
  height: 200rpx;
  left: 0;
  top: 0;
  z-index: 1;
}
.haeder .biao {
  position: absolute;
  z-index: 2;
  right: 70rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 100rpx;
  height: 100rpx;
}
.haeder .info {
  position: relative;
  color: #ffffff;
  z-index: 3;
  padding-left: 40rpx;
}
.haeder .info .title {
  font-size: 32rpx;
  margin-bottom: 10rpx;
}
.haeder .info .addresss {
  font-size: 24rpx;
}
.list {
  margin-top: 20rpx;
  padding: 20rpx;
  text-align: center;
  background: #ffffff;
  padding-bottom: 50rpx;
}
.list .disc {
  font-size: 32rpx;
  margin-bottom: 30rpx;
}
.list .table {
  border-right: 1px solid #999;
  border-left: 1px solid #999;
  border-top: 1px solid #999;
  font-size: 24rpx;
}
.list .table .table-item {
  display: flex;
  border-bottom: 1px solid #999;
  height: 70rpx;
  align-items: center;
}
.list .table .table-item .key {
  border-right: 1px solid #999;
  width: 30%;
  height: 100%;
  line-height: 70rpx;
}
.list .table .table-item .val {
  width: 70%;
  height: 100%;
  line-height: 70rpx;
}
.no-default {
  text-align: center;
  color: #999999;
  padding-top: 40rpx;
  height: 100vh;
  background: #ffffff;
  margin-top: 0 !important;
}
.no-default image {
  width: 500rpx;
  height: 300rpx;
  margin-top: 100rpx;
}
.table-list {
  padding: 50rpx 20rpx;
  background: #ffffff;
  text-align: center;
  margin-top: 20rpx;
}
.table-list .title {
  margin-bottom: 25rpx;
  font-size: 36rpx;
}
.table-list .header {
  display: flex;
  background: rgba(115, 171, 255, 0.5);
  line-height: 70rpx;
  text-align: center;
  color: #ffffff;
  margin-top: 30rpx;
}
.table-list .header .item {
  width: 20%;
  flex-shrink: 0;
  border-right: 1rpx solid #eee;
}
.table-list .header .nowrap {
  white-space: nowrap;
}
.table-list .table-item {
  display: flex;
  background: #ffffff;
  min-height: 70rpx;
  align-items: center;
  text-align: center;
  color: #333333;
  border-bottom: 1rpx solid #eee;
}
.table-list .table-item .item {
  width: 20%;
  flex-shrink: 0;
  word-break: break-all;
  border-right: 1rpx solid #eee;
}
.table-list .table-item .item:nth-child(1) {
  border-left: 1rpx solid #eee;
}
.table-list .table-item .item2 {
  flex: 1;
  word-break: break-all;
  border-right: 1rpx solid #eee;
}
.table-list .table-item .item2:nth-child(1) {
  border-left: 1rpx solid #eee;
}
