<!--pages/xianchengda/xianchengda.wxml-->
<view class="map-container">
  <map id="map" longitude="{{lng}}" latitude="{{lat}}" scale="{{scale}}" controls="{{controls}}"
       markers="{{markers}}" bindmarkertap="markertap" polyline="{{polyline}}"
       bindregionchange="regionchange" show-location style="width: 100%; height: 100%;"></map>
</view>
